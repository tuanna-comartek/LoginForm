{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier",
    "tslint-config-airbnb",
    "tslint-react"
  ],
  "defaultSeverity": "error",
  "jsRules": {},
  "linterOptions": {
    "exclude": [
      "bin",
      "generated/*/**.ts",
      "**/*.js"
    ]
  },
  "rules": {
    "jsx-self-close": true,
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "function-name": false,
    "ordered-imports": false,
    "import-name": false,
    "object-literal-sort-keys": false,
    "arrow-parens": false,
    "no-this-assignment": false,
    "no-console": [
      false
    ],
    "no-unused-expression": true,
    "object-shorthand-properties-first": false,
    "comment-format": false,
    "semicolon": false,
    "ter-arrow-parens": false,
    "no-parameter-reassignment": false,
    "no-var-requires": false,
    "no-string-literal": false,
    "array-type": false,
    "no-empty-interface": false,
    "prefer-array-literal": false,
    "jsx-boolean-value": false,
    "ter-indent": false,
    "jsx-curly-spacing": false,
    "space-before-function-paren": false,
    "ter-func-call-spacing": false,
    "prefer-for-of": false,
    "no-increment-decrement": false,
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "ignore",
          "typeLiterals": "ignore"
        }
      }
    ],
    "no-shadowed-variable": false,
    "align": false,
    "ban": false,
    "class-name": true,
    "curly": false,
    "eofline": true,
    "forin": true,
    "indent": [
      true,
      "spaces",
      2
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "jsdoc-format": true,
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "react/jsx-boolean-value": true,
    "label-position": true,
    "max-line-length": [
      true,
      400
    ],
    "member-ordering": false,
    "member-access": false,
    "no-trailing-whitespace": false,
    "no-any": false,
    "no-arg": true,
    "no-switch-case-fall-through": true,
    "switch-default": true,
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "typedef": [
      true,
      "parameter",
      "property-declaration"
    ],
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-module",
      "check-operator",
      "check-separator",
      "check-type",
      "check-typecast"
    ]
  },
  "rulesDirectory": []
}
